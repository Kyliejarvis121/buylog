generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model banners {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model carts {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model categories {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  /// Field referred in an index, but found no data to define the type.
  slug Json?  @unique(map: "slug_1")
}

model homebanners {
  id     String   @id @default(auto()) @map("_id") @db.ObjectId
  v      Int      @map("__v")
  images String[]
}

model homebottombanners {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model homesidebanners {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model imageuploads {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model mylists {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model orders {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model productrams {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model productreviews {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model products {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model productsizes {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model productweights {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model recentlyviewds {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model users {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  v          Int      @map("__v")
  date       DateTime @db.Date
  email      String
  /// Could not determine type: the field only had null or empty values in the sample set.
  images     Json?
  isAdmin    Boolean
  isVerified Boolean
  name       String
  /// Could not determine type: the field only had null or empty values in the sample set.
  otp        Json?
  /// Could not determine type: the field only had null or empty values in the sample set.
  otpExpires Json?
  password   String
  phone      String
}
